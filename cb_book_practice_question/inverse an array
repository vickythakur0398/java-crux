package array;

public class inverse_an_array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {3, 4, 0, 1, 2};
		inverse(arr);
		
	}public static int[] inverse(int[] arr) {
		int [] new_arr = new int[arr.length];
		for(int i =0; i<arr.length; i++) {
			new_arr[arr[i]] = i;			
		}
		
		
		return new_arr;
	
	}

}
